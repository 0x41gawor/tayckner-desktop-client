        <Insets left="10.0" right="10.0"/>
    </padding>
    <Label fx:id="label_name" prefHeight="30.0" prefWidth="787.0" text="Work">
        <font>
          <Font size="20.0"/>
        </font>
    </Label>
    <Label fx:id="label_time" alignment="CENTER" prefHeight="30.0" prefWidth="129.0" text="9:00 - 12:00">
        <font>
          <Font size="24.0"/>
        </font>
    </Label>
    <Label fx:id="label_duration" alignment="CENTER" prefHeight="30.0" prefWidth="59.0" text="3h">
        <font>
          <Font size="20.0"/>
        </font>
    </Label>
</HBox>
